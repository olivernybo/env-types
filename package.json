    "typed",
    "convert",
    "conversion",
    "parse",
    "parsing",
    "strong",
    "strict"
  ],
  "author": "Oliver Nybo <oliver@nybo.dev>",
  "license": "MIT",
  "repository": "github:olivernybo/env-types",
